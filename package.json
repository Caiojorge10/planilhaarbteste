{
  "name": "planilha-arbitragem-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de arbitragem",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npx prisma generate --schema=./prisma/schema.prisma && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "seed": "ts-node src/seedWithData.ts",
    "deploy": "npm install && npx prisma generate --schema=./prisma/schema.prisma && npx prisma db push --schema=./prisma/schema.prisma && npm run build && npm run seed"
  },
  "dependencies": {
    "@prisma/client": "^6.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.5",
    "@types/sqlite3": "^3.1.11",
    "prisma": "^6.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
